.content {
  position: relative;
  width: 100%;
  height: 100vh;
  overflow: hidden;
}
.content map {
  position: relative;
  width: 100%;
  height: 100vh;
}
.content .search {
  position: absolute;
  left: 40rpx;
  top: 50rpx;
  width: 650rpx;
  height: 70rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  background: #fff;
  border-radius: 70rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.content .search button {
  height: 70rpx;
  background: #fff;
  color: #333;
  font-size: 28rpx;
}
.content .search button:after {
  border: none;
}
.content .search button.selected {
  color: #f6761c;
  font-weight: bold;
}
.content .cover-info {
  position: absolute;
  left: 0;
  bottom: 0;
  background: #fff;
  width: 100%;
  height: 50vh;
  box-shadow: 0 1px 1px 1px #333;
  overflow: hidden;
}
.content .cover-info button:after {
  border: none;
}
.content .cover-info .close {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat 50% 50%;
  background-size: 30rpx 30rpx;
  outline: none;
  border: none;
  z-index: 3;
}
.content .cover-info .tabs {
  position: absolute;
  top: 10rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  height: 60rpx;
  width: 400rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  z-index: 4;
}
.content .cover-info .tabs button {
  width: 160rpx;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #666;
}
.content .cover-info .tabs button.selected {
  background: #71d5a1;
  color: #fff;
}
.content .cover-info .content {
  box-sizing: border-box;
  width: 100%;
  height: 50vh;
  padding: 80rpx 20rpx 20rpx;
  overflow-y: scroll;
}
.content .cover-info .content .h1-title {
  line-height: 50rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #000;
  margin-bottom: 10rpx;
  white-space: pre-wrap;
  margin-bottom: 30rpx;
}
.content .cover-info .content .hine {
  line-height: 40rpx;
  font-size: 24rpx;
  color: #999;
  margin-bottom: 20rpx;
}
.content .cover-info .content .introduction {
  padding: 20rpx;
  border-radius: 20rpx;
  background: #f5f5f5;
  line-height: 46rpx;
  font-size: 28rpx;
  white-space: pre-wrap;
  margin-bottom: 20rpx;
}
.content .cover-info .content .introduction .zone {
  line-height: 46rpx;
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.content .cover-info .content .introduction .treat {
  font-size: 26rpx;
  line-height: 36rpx;
  color: #fa4630;
}
.content .cover-info .content .confirmed-view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 30rpx;
  line-height: 60rpx;
  height: 60rpx;
  margin-bottom: 10rpx;
  border-radius: 10rpx;
}
.content .cover-info .content .confirmed-view .city-name {
  padding-left: 20rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  color: #000;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
  background: #f5f5f5;
}
.content .cover-info .content .confirmed-view .confirmed {
  box-sizing: border-box;
  padding-left: 20rpx;
  width: 200rpx;
  line-height: 60rpx;
  background: #f7e8ea;
  color: #ae212c;
}
.content .cover-info .content .confirmed-view .new-num {
  box-sizing: border-box;
  padding-left: 20rpx;
  width: 200rpx;
  line-height: 60rpx;
  background: #fcece9;
  color: #e64426;
}
.content .cover-info .content .header {
  padding: 10rpx 20rpx 30rpx;
  border-bottom: 1rpx solid #eee;
}
.content .cover-info .content .header .h1 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  line-height: 50rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #000;
  margin-bottom: 10rpx;
  white-space: pre-wrap;
}
.content .cover-info .content .header .h1 .mark {
  background: #fa4630;
  color: #fff;
  font-size: 24rpx;
  line-height: 36rpx;
  padding: 0 10rpx;
  border-radius: 8rpx;
  margin-left: 14rpx;
}
.content .cover-info .content .header .h3 {
  line-height: 40rpx;
  font-size: 28rpx;
  color: #666;
  white-space: pre-wrap;
}
.content .cover-info .content .ul {
  padding: 20rpx 20rpx 40rpx;
}
.content .cover-info .content .ul .title-no {
  width: 110rpx;
  padding: 0 10rpx;
  font-size: 24rpx;
  line-height: 44rpx;
  color: #ff9158;
  background: #fff7f2;
  border-radius: 4rpx;
  text-align: center;
}
.content .cover-info .content .ul .title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 24rpx;
  line-height: 36rpx;
  padding-bottom: 10rpx;
  color: #666;
}
.content .cover-info .content .ul .title .manlen {
  font-size: 24rpx;
  font-weight: bold;
  line-height: 36rpx;
  color: #333;
  font-weight: bold;
  padding: 0 3rpx;
}
.content .cover-info .content .ul .li {
  margin-bottom: 20rpx;
}
.content .cover-info .content .ul .li .man-info {
  font-size: 30rpx;
  font-weight: bold;
  line-height: 50rpx;
  color: #333;
}
.content .cover-info .content .ul .li .p {
  line-height: 36rpx;
  font-size: 24rpx;
  color: #666;
  padding: 10rpx 0;
  white-space: pre-wrap;
}
.content .cover-info .content .ul .li .track-list {
  padding: 20rpx;
  border-radius: 20rpx;
  background: #f5f5f5;
}
.content .cover-info .content .ul .li .track-list .track {
  padding: 10rpx 0 18rpx;
  line-height: 46rpx;
  font-size: 28rpx;
  color: #666;
  white-space: pre-wrap;
}
.content .cover-info .content .ul .li .track-list .track .timer {
  display: inline;
  color: #333;
  font-weight: bold;
  line-height: 40rpx;
}
.content .cover-info .content .ul .li .track-list .track:last-child {
  border: none;
}
.content .cover-info .content .nothing {
  padding: 100rpx 0;
  text-align: center;
  color: #666;
  font-size: 30rpx;
}

